.vc_logo {
	display: none !important;
}

.dt_image {
	max-height: 100px;
	max-width: 90%;
	width: auto;
	margin: 10px 0;
	/* border: 1px solid #EBEBEB; */
	/* padding: 5px; */
	border-radius: 3px;
	-webkit-border-radius: 3px;
}


[class*="dt_vc_ico"] {
	margin-top: -1px;
	height: 16px;
	width: 16px;
	background-position: center center;
	background-repeat: no-repeat;
}
[class*="dt_vc_sc"].wpb_content_element > .wpb_element_wrapper {
	height: auto;
	background-position:  21px center;
	background-repeat: no-repeat;
}
	[class*="dt_vc_sc"].wpb_content_element .wpb_vc_param_value.content ul {
		margin-left: 15px;
		list-style: disc;
	}
	[class*="dt_vc_sc"].wpb_content_element .vc_admin_label {
		display: block;
		clear: both;
	}

.dt_vc_ico_quote,
.dt_vc_sc_quote.wpb_content_element > .wpb_element_wrapper  {
	background-image: url("../includes/quote/quotes.png");
}

